    "servers": [
      {
        "url": "https://banking-app-fis7hiuba-isaac-svg.vercel.app/api-docs/"
      }
    ],
    "paths": {
      "/auth/register": {
        "post": {
          "summary": "Register endpoint",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "first_name": {
                      "type": "string",
                      "example":"Isaac"
                    },
                    "last_name": {
                      "type": "string",
                      "example":"Sakyi"
                    },
                    "date_of_birth": {
                      "type": "string",
                      "example":"1990-09-09"
                    },
                    "phone": {
                      "type": "string",
                      "example":"+233 59 1514 584"
                    },
                    "email": {
                      "type": "string",
                      "example":"isaacsakyi009@gmail.com"
                    },
                    "address": {
                      "type": "string",
                      "example":"New York"
                    },
                    "ghana_card_number": {
                      "type": "string",
                      "example":"GHA-2345-333"
                    },
                    "password": {
                      "type": "string",
                      "example":"password"
                    }
                  },
                  "required": ["first_name", "last_name", "email", "password","ghana_card_number","address","date_of_birth","phone"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Account created Successffully"
            }
          }
        }
      },
      "/auth/login": {
        "post": {
          "summary": "Login endpoint",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "first_name": {
                      "type": "string",
                      "example":"Isaac"
                    },
                    "last_name": {
                      "type": "string",
                      "example":"Sakyi"
                    },
                    "email": {
                      "type": "string",
                      "example":"isaacsakyi009@gmail.com"
                    },
                    "password": {
                      "type": "string",
                      "example":"password"
                    }
                  },
                  "required": ["email", "password"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Login Successfull"
            }
          }
        }
      },
      "/auth/logout": {
        "post": {
          "summary": "Logout endpoint",
          "responses": {
            "200": {
              "description": "Logout Successful"
            }
          }
        }
      },
      "/auth/profile": {
        "get": {
          "summary": "Get profile endpoint",
          "responses": {
            "200": {
              "description": "Successful response"
            }
          }
        }
      },
      "/transaction/checkbalance": {
        "get": {
          "summary": "Check balance endpoint",
          "responses": {
            "200": {
              "description": "Successful response"
            }
          }
        }
      },
      "/sudo/allcustomers": {
        "get": {
          "summary": "Get all customers endpoint",
          "responses": {
            "200": {
              "description": "Successful response"
            }
          }
        }
      },
      "/transaction/transfer": {
        "post": {
          "summary": "Transfer endpoint",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "from_account_number": {
                      "type": "string",
                      "example":"1402222550431738"
                    },
                    "to_account_number": {
                      "type": "string",
                      "example":"1402222550431738"
                    },
                    "amount": {
                      "type": "string",
                      "example":"100"
                    }
                  },
                  "required": ["from_account_number", "to_account_number", "amount"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful response"
            }
          }
        }
      },
      "/get/profile": {
        "get": {
          "summary": "Get profile endpoint",
          "responses": {
            "200": {
              "description": "You are authorized"
            }
          }
        }
      },
      "/get/account_number": {
        "get": {
          "summary": "Get account number endpoint",
          "parameters": [
            {
              "in": "query",
              "name": "account_number",
              "schema": {
                "type": "string",
                "example":"1402222550431738"
              },
              "required": true
            }
          ],
          "responses": {
            "200": {
              "description": "Successful response"
            }
          }
        }
      }
    }
  }
  