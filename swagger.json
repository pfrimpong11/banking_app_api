{
    "openapi": "3.0.0",
    "info": {
      "version": "1.0.0",
      "title": "Banking Api",
      "description": "This api provides the basic functionality that a banking application provides",
      "contact": {
        "name": "Isaac Sakyi",
        "email": "isaacsakyi009@gmail.com"
